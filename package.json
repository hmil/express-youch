    },
    "devDependencies": {
        "@senhung/http-exceptions": "1.1.1",
        "@types/chai": "4.2.8",
        "@types/express": "4.16.0",
        "@types/inquirer": "6.5.0",
        "@types/mocha": "7.0.1",
        "@types/shelljs": "0.8.6",
        "@types/supertest": "2.0.8",
